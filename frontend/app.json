{
  "expo": {
    "name": "AayuCare",
    "slug": "aayucare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/aayucare-logo.png",
    "userInterfaceStyle": "light",
    "scheme": "aayucare",
    "splash": {
      "image": "./assets/images/aayucare-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A90E2"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aayucare.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ]
      },
      "jsEngine": "hermes"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/aayucare-logo.png",
        "backgroundColor": "#4DD0E1"
      },
      "package": "com.aayucare.app",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "jsEngine": "hermes"
    },
    "updates": {
      "enabled": false,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0
    },
    "extra": {
      "sentryDSN": {},
      "API_BASE_URL": "https://aayucare-backend.onrender.com/api",
      "PRODUCTION_API_URL": "https://aayucare-backend.onrender.com/api",
      "eas": {
        "projectId": "31972ca4-e87a-4c14-862f-330973332013"
      }
    },
    "plugins": [
      "expo-asset"
    ]
  }
}
